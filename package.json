{
  "name": "Quizz",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/server/index.js",
  "scripts": {
    "start": "node ./dist/server/index.js",
    "dev:server": "npx nodemon --exec npx ts-node ./server/index.ts --watch *.ts",
    "dev:client": "npx parcel ./client/src/{playground,index}.html --open",
    "build": "npx rimraf ./dist && yarn build:server && yarn build:client",
    "build:server": "npx tsc",
    "build:client": "npx parcel build --no-source-maps ./client/src/index.html -d ./dist/client",
    "g": "node ./scripts/generateCode",
    "postinstall": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@testing-library/react": "^8.0.8",
    "@types/express": "^4.17.0",
    "@types/prop-types": "^15.7.1",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "axios": "^0.21.1",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "parcel-bundler": "^1.12.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "tsc": "^1.20150623.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.16",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "sass": "^1.22.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
